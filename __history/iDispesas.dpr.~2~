program iDispesas;

uses
  Vcl.Forms,
  ULogin in 'ULogin.pas' {FormLogin},
  UCadastro in 'UCadastro.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormLogin, FormLogin);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
